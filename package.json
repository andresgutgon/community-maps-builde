{
  "name": "coopdevs-community-maps",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint --fix",
    "run_prettier": "npx prettier --write .",
    "prettier_script": "npx prettier --check .",
    "lint_and_tsc": "yarn prettier_script && yarn lint && yarn tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "extract:i18n": "formatjs extract 'src/{pages,hooks,components}/**/*.{js,ts,tsx}' --out-file 'content/locales/es.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile:i18n": "formatjs compile-folder --ast content/locales content/compiled-locales",
    "i18n": "yarn extract:i18n && yarn compile:i18n",
    "build": "yarn i18n && next build",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@jsonforms/core": "3.0.0-beta.1",
    "@jsonforms/react": "3.0.0-beta.1",
    "@jsonforms/vanilla-renderers": "3.0.0-beta.1",
    "@reach/slider": "^0.16.0",
    "@react-aria/focus": "^3.5.0",
    "ajv": "^8.6.1",
    "classnames": "^2.3.1",
    "leaflet": "1.7.1",
    "leaflet.markercluster": "^1.5.3",
    "leaflet-control-geocoder": "^2.4.0",
    "marked": "^4.0.7",
    "next": "12.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "^5.22.0",
    "react-leaflet": "^3.2.2",
    "react-leaflet-markercluster": "^3.0.0-rc1",
    "sanitize-html": "^2.6.1",
    "screenfull": "^6.0.0"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.4.0",
    "@formatjs/ts-transformer": "^3.6.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/leaflet": "^1.7.6",
    "@types/leaflet.markercluster": "^1.4.6",
    "@types/marked": "^4.0.1",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.38",
    "@types/react-leaflet": "^2.8.2",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.3.1",
    "eslint": "<8.0.0",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-formatjs": "^2.18.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^12.2.1",
    "next-test-api-route-handler": "^2.3.4",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "prettier": "2.5.1",
    "tailwindcss": "^3.0.13",
    "ts-jest": "^27.0.7",
    "typescript": "<4.5.0"
  }
}
