{
  "suggest_place_generic": {
    "slug": "suggest_place_generic",
    "formButtonLabel": "Sugerir Lugar",
    "description": "Sugierenos un lugar",
    "jsonSchema": {
      "type":"object",
      "properties":{
        "name":{
          "type":"string"
        },
        "email":{
          "type":"string",
          "format": "email"
        }
      },
      "required": ["name", "email"]
    },
    "uiSchema": {
      "type":"VerticalLayout",
      "elements":[
        {
          "type":"Control",
          "scope":"#/properties/name",
          "label": "Nombre"
        },
        {
          "type":"Control",
          "scope":"#/properties/email",
          "label":"Email"
        }
      ]
    }
  },
  "van": {
    "slug": "van",
    "formButtonLabel": "Sugerir un lugar",
    "description": "Sugierenos un lugar para Furgonetas",
    "jsonSchema": {
      "type":"object",
      "properties":{
        "suggestedAddress": {
          "type": "object",
          "geoAddress": true,
          "properties": {
            "address": { "type": "string" },
            "latitude": { "type": "string" },
            "longitude": { "type": "string" }
          },
          "required": ["latitude", "longitude"]
        },
        "email":{
          "type":"string",
          "format": "email"
        }
      },
      "required": ["email", "suggestedAddress"]
    },
    "uiSchema": {
      "type":"VerticalLayout",
      "elements":[
        {
          "type":"Control",
          "scope":"#/properties/suggestedAddress",
          "label":"Direcci√≥n del lugar"
        },
        {
          "type":"Control",
          "scope":"#/properties/email",
          "label":"Email"
        }
      ]
    }
  }
}

